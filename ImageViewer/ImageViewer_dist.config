    <ClearCanvas.ImageViewer.Layout.Basic.DefaultPatientReconciliationSettings>
      <!--
      IMPORTANT: do not modify these rules unless you are very certain you know what you are doing.
      These rules essentially edit patient IDs for display in the viewer right-click context menu
      and have the potential to cause different patients to be unified under a single patient ID.
      Please be sure to thoroughly test any rules you have written.
      -->
      <setting name="SearchReconciliationRulesXml" serializeAs="Xml">
        <value>
          <search-reconciliation-rules>
            <!--<rule id="Site Suffix">
              <condition>
                <regex test="$.PatientId" pattern="\A\d+[\s\d]*\D+\s*\z"/>
              </condition>
              <actions>
                <replace-regex property="PatientId" pattern="\s*\D+\s*\z" replacement="*" />
              </actions>
            </rule>-->
            <rule id="Default">
              <condition/>
              <actions>
                <replace-regex property="PatientId" pattern="\s*\z" replacement="*" />
              </actions>
            </rule>
          </search-reconciliation-rules>
        </value>
      </setting>
      <setting name="PatientReconciliationRulesXml" serializeAs="Xml">
        <value>
          <patient-reconciliation-rules>
            <!--<rule id="UHN">
              <condition>
                <regex test="$.PatientId" pattern="\A\d{7}\s*(UHN|PMH|TGH|TWH)\s*\z"/>
              </condition>
              <actions>
                <replace-regex property="PatientId" pattern="\s*(UHN|PMH|TGH|TWH)\s*\z" replacement="" />
              </actions>
            </rule>
            <rule id="MSH">
              <condition>
                <regex test="$.PatientId" pattern="\A\d{3}\s\d{3}\s\d{3}\s*MSH\s*\z"/>
              </condition>
              <actions>
                <replace-regex property="PatientId" pattern="\s*MSH\s*\z" replacement="" />
              </actions>
            </rule>
            <rule id="WCH">
              <condition>
                <regex test="$.PatientId" pattern="\A\d{7}WCH\s*\z"/>
              </condition>
              <actions>
                <replace-regex property="PatientId" pattern="WCH\s*\z" replacement=" WCH" />
              </actions>
            </rule>-->
            <!--
            The following xml can be used to specify a set of rules that can be applied to patient information
            in order to unify the same patient's information in the viewer (e.g. context menu and thumbnails).
            Here is an example of something you can do (output: WHO WANTS ICE CREAM?)

            <rule id="WHO WANTS ICE CREAM">
                <condition>
                  <regex test="$.PatientId" pattern="\d{7}\s(UHN|PMH|TGH|TWH)\z"/>
                </condition>
                <actions>
                  <replace-regex property="PatientId" pattern="\d{7}" replacement="WANTS" />
                  <replace-regex property="PatientId" pattern="(UHN|PMH|TGH|TWH)" replacement=" ICE" />
                  <prepend property="PatientId" value="XX" />
                  <replace property="PatientId" occurrence="XX" replacement="xxWHO " />
                  <trim-start property="PatientId" characters="2" />
                  <append property="PatientId" value=" CREAMxx" />
                  <trim-end property="PatientId" characters="2" />
                  <append property="PatientId" value="?" />
            </actions>
            </rule>
            -->
          </patient-reconciliation-rules>
        </value>
      </setting>
    </ClearCanvas.ImageViewer.Layout.Basic.DefaultPatientReconciliationSettings>
